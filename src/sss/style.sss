input.excursion-btn
    display: block
    margin: 1.2vw auto 0
    width: 10vw
    border: 1px solid $color-title
    border-radius: 5vw
    &:hover
        background: $color-title
        color: white
        font-weight: $medium
    &:active
        background: color($color-title tint(15%))
    @media (--mobile)
        padding: 2vw
        margin-top: 4.8vw
        width: 40vw
        border: 1px solid $color-title
        border-radius: 10vw

.excursion-search-count
    text-align: center
    margin-top: 1vw
    margin-bottom: 1vw
    color: white
    font-size: 0.9vw
    @media (--mobile)
        margin: 2vh auto
        font-size: 14px
        color: $color-main

.excursion-block-p
    margin-bottom: 0.2vw
    color: $color-main
    line-height: 1.2vw
    font-weight: $light
    font-size: 0.9rem
    @media (--mobile)
        color: $color-main-mobile
        line-height: 150%
        font-size: 14px

.excursion-block-h
    margin-top: 1vw
    margin-bottom: 0.2vw
    color: $color-title
    text-transform: uppercase
    font-weight: $bold
    font-size: 1.2rem
    @media (--mobile)
        margin-bottom: 1vw
        font-size: 18px

.excursion-block-h.expired
    color: $color-title-negative

.excursion-block-h2
    margin-bottom: 0.2vw
    color: $color-main
    text-transform: uppercase
    font-weight: $bold
    font-size: 0.9rem
    @media (--mobile)
        margin-bottom: 1vw
        font-size: 14px

.excursion-block-t,
.excursion-block-t a
    width: 20%
    color: $color-title
    font-weight: $bold
    font-size: 0.9rem
    @media (--mobile)
        width: 50%
        font-size: 14px

.excursion-td-span
    text-transform: uppercase
    font-weight: $bold

.news-block
    margin-bottom: 3vw

.rkg-news-category-select
    @media (--desktop)
        display: none
    @media (--mobile)
        .rkg-news-category-select-arrow
            @mixin arrow bottom, $color-main, 14px, 1px
        .rkg-news-category-select-arrow.up
            @mixin arrow top, $color-main, 14px, 1px

.rkg-news-category,
a.rkg-news-category-link
    color: $color-main
    cursor: pointer
    &:hover
        color: color($color-main a(80%))
    &.active
        color: $color-title
    @media (--mobile)
        margin-top: 14px
        display: none

.rkg-category-container
    display: flex
    justify-content: space-between
    width: 60vw !important
    @media (--mobile)
        flex-direction: column
        text-align: center

.news-block-item
    width: 100%
    height: 0
    padding-top: 60%
    display: block
    position: relative
    margin-bottom: 3.2vw
    background-position: center
    background-repeat: no-repeat
    background-size: cover
    border-radius: 1.9vw
    transition: background-size 0.2s ease-in
    @media (--mobile)
        margin-bottom: 12vw
    &:hover
        background-size: 120%

.news-block-category
    @util position(absolute, 1.5vw null null 1vw)
    color: white
    text-transform: uppercase
    font-weight: $bold
    font-size: 0.9rem
    text-shadow: 0 0 1px black, 0 0 1px black
    @media (--mobile)
        top: 20px
        font-size: 14px

.news-block-title
    @util position(absolute, null null 0.5vw 1vw)
    color: white
    text-transform: uppercase
    font-weight: $bold
    font-size: 0.9rem
    text-shadow: 0 0 1px black, 0 0 1px black
    @media (--mobile)
        font-size: 14px
        bottom: 0

.news-block-date
    @util position(absolute, null null -1.2vw 1vw)
    color: $color-main
    font-weight: $bold
    font-size: 0.9rem
    @media (--mobile)
        bottom: -20px
        font-size: 14px

.news-block-filter
    @util position(absolute, 0 0 0 0)
    background: $color-title
    border-radius: 1.9vw
    opacity: 0.1

.salvattore[data-columns]
    @util clearfix
    margin-right: -2% !important
    margin-left: -2% !important
    &::before
        content: '3 .column.size-1of3'

.column
    float: left

.size-1of3
    width: 30%
    margin: 0 1.6%

#brevet
    display: none

.rkg-croppie-container
    @util size(320px, 363px)
    margin: 0 auto 100px
    input
        width: 200px
    @media (--mobile)
        margin: 0 auto 32px
        input
            border: none
            padding: 5px
            width: 250px
        .cr-slider-wrap
            margin: 5px auto

.croppie-rotate
    cursor: pointer
    margin: 0 8vw
    @media (--mobile)
        font-size: 32px
        margin: 1vh 8vh 3vh

form#helth-survey,
form#responsibility-survey
    padding: 0.5vw
    h2
        text-align: center
        font-weight: $semi-bold
        font-size: 1.6rem
        color: $color-title
    h3
        text-align: center
        font-weight: $regular
        font-size: 1.6rem
        color: $color-title
    p
        text-align: left
        font-size: 0.9rem
        color: $color-main
    p.center
        text-align: center
    td
        text-align: left
        font-size: 0.9rem
        color: $color-main
        padding-bottom: 1vw
        vertical-align: top
        &:first-child
            padding-right: 1vw
    ul
        li
            text-align: left
            font-size: 0.9rem
            color: $color-main
    @media (--mobile)
        h2,
        h3
            font-size: 18px
        p,
        td
            font-size: 14px
        ul
            li
                font-size: 14px


.leaflet-popup-bold-now,
.leaflet-popup-bold-new
    color: $color-title

.leaflet-popup-bold-old
    color: $color-title-negative

.mobile-drop
    @media (--mobile)
        display: none

.rkg-nowrap
    white-space: nowrap

.rkg-toggler
    display: none
    @media (--mobile)
        display: block
        cursor: pointer

.rkg-toggler-title
    span
        display: none
    @media (--mobile)
        cursor: pointer
        span
            display: inline

.rkg-hidder
    @media (--mobile)
        display: none
