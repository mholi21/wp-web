/*!
 * Theme Name RK Geronimo theme
 * Description Starter Theme to use with Timber
 * Author Upstatement and YOU!
 */

@import '_variables.sss'
@import '_mixins.sss'
@import '_icons.sss'
@import '_calendar.sss'

*
    font-family: 'Montserrat', sans-serif

body
    margin: 0
    padding: 0
    color: $color-main
    font-weight: $light
    font-size: 1.5rem
    &.modal-open
        /* overflow: hidden */
    @media (--mobile)
        /* line-height: 1500% */
        font-size: 14px

main
    margin: 0
    padding: 0


.only-desktop
    @media (--mobile)
        display: none

.only-mobile
    @media (--desktop)
        display: none

article
    @util clearfix

input[type=submit],
button
    background: white
    cursor: pointer

.button
    cursor: pointer

.rkg-meni-background
    @util position(fixed, 0 0 0 0)
    z-index: 1009
    display: none
    background-color: black
    opacity: 0.95

.rkg-modal-background
    @util position(fixed, 0 0 0 0)
    z-index: 1010
    display: none
    background-color: black
    opacity: 0.5

.modal-dob-container
    margin: auto
    width: 22vw
    display: flex
    justify-content: space-between
    @media (--mobile)
        width: 68vw

.modal-dob
    font-size: 0.9vw
    border-radius: none
    padding: 1vw
    border: 1px solid color($color-main tint(50%))
    @media (--mobile)
        font-size: 14px
        padding: 4vw

.rkg-modal
    @util position(fixed, 37.5% null null 50%)
    transform: translate(-50%, -50%)
    z-index: 1020
    display: none
    padding: 2vw 5vw
    background: white
    text-align: center
    min-width: 30vw
    transition: width 0.5s, height 0.2s
    .form-horizontal
        max-height: 75vh
        overflow-y: auto
    .form-hide
        display: none
    p
        margin-bottom: 1vw
        font-size: 0.9rem
        &:first-child
            color: $color-title
            font-size: 1.6rem
        .rkg-modal-link
            color: $color-title
            cursor: pointer
    a
        color: $color-title
        text-decoration: none
    input
        font-size: 0.9rem
        padding: 1vw
        width: 20vw
        border: 1px solid color($color-main tint(50%))
        &::placeholder
            color: $color-main
            font-weight: $medium
        &[type=submit]
            margin-top: 1vw
            width: 10vw
            border: 1px solid $color-title
            border-radius: 5vw
            &:hover
                background: $color-title
                color: white
                font-weight: $medium
            &:active
                background: color($color-title tint(15%))
    .rkg-button
        display: inline-block
        color: $color-main
    button,
    .rkg-button
        font-size: 0.9rem
        padding: 1vw
        margin-top: 1vw
        width: 10vw
        border: 1px solid $color-title
        border-radius: 5vw
        &:hover
            background: $color-title
            color: white
            font-weight: $medium
        &:active
            background: color($color-title tint(15%))
    input[type="checkbox"]
        width: auto
        margin: 0
    .rkg-input-size
        text-align: left
        display: inline-block
        width: 22vw
    .rkg-modal-status
        display: block
        height: 2vw
        &.error
            color: $color-title-negative
        &.ok
            color: $color-title
    @media (--mobile)
        @mixin center-vertical
        width: 100vw
        box-sizing: border-box
        padding: 0
        @util position(fixed, 0 0 0 0)
        transform: none
        p
            margin-bottom: 3vw
            font-size: 14px
            &:first-child
                font-size: 23px
        input
            font-size: 14px
            padding: 4vw
            width: 60vw
            &[type=submit]
                margin-top: 2vw
                width: 40vw
                border: 1px solid $color-title
                border-radius: 10vw
        button
            font-size: 14px
            padding: 2vw
            margin-top: 2vw
            width: 30vw
            border: 1px solid $color-title
            border-radius: 10vw
        input[type="checkbox"]
            width: auto
        .rkg-input-size
            display: inline-block
            width: 64vw
        form
            width: 98%

.rkg-modal-close
    @mixin x $color-main, 1.5vw
    @util position(absolute, 1vw 1vw null null)
    @media (--mobile)
        @mixin x $color-main, 7vw, 0.5vw
        @util position(absolute, 5vw 5vw null null)

.rkg-info-close
    display: none
    @media (--mobile)
        display: block
        @mixin x $color-main, 7vw, 0.5vw
        @util position(absolute, 5vw 5vw null null)

.rkg-modal-logo
    display: none
    @media (--mobile)
        display: block
        @util position(absolute, 0 null null 0)

.rkg-modal-flex
    @media (--mobile)
        height: 100%
        width: 100%
        display: flex
        flex-direction: column
        align-items: center
        justify-content: center

.indicator
    display: none

.rkg-meni
    @util position(fixed, -50% 0 50% 0)
    z-index: 1010
    display: none
    overflow: hidden
    color: $color-header

.rkg-meni-title
    display: block
    margin: 8vw auto 6vw
    color: $color-header
    text-align: center
    text-decoration: none
    text-transform: uppercase
    font-weight: $bold
    font-size: 2.2rem
    @media (--mobile)
        margin: 20vw auto
        font-size: 23px

.rkg-meni-external
    @util position(absolute, null null 2.5vw null)
    width: 100%
    text-align: center
    font-size: 3.5vw
    a
        margin: 0 1.2vw
        color: $color-header
        &:hover
            color: color($color-header a(80%))
    @media (--mobile)
        margin-bottom: 4vh
        a
            margin: 0 5vw
            font-size: 30px

.rkg-profile-meni
    @util position(fixed, 6vw 5vw null null)
    z-index: 1020
    display: none
    width: 15vw
    background: white
    box-shadow: 0 4px 8px 0 color(black a(50%)), 0 6px 20px 0 color(black a(49%))
    > div,
    a
        padding: 1vw
        border-bottom: 1px solid $color-main
        font-size: 0.9rem
    ul
        list-style: none
        display: none
        margin: 0
        padding: 0
        border-bottom: 1px solid $color-main
    li
        a
            padding-left: 3vw
    div
        p:first-child
            color: $color-title
            font-weight: $medium
    a
        display: block
        color: $color-title
        text-decoration: none
        font-weight: $medium
        &:last-child
            border-bottom: none
        &:hover
            background: color(white shade(15%))
        &:active
            background: color(white shade(25%))
        &::before
            padding-right: 0.5vw

        &.ShowPayModal
            color: #c80000
    .rkg-profile-meni-controls
        display: none
    > .rkg-profile-meni-item-group
        padding: 0
    @media (--mobile)
        box-shadow: none
        @util position(fixed, 0 null 0 -90vw)
        display: block
        padding-top: 18vw
        width: 90vw
        overflow-y: auto
        transition: left 0.2s
        > div,
        a
            padding: 2vh
            font-size: 14px
        a::before
            padding-right: 6vw
        .rkg-profile-meni-controls
            display: block
            @util position(absolute, 0 0 null 0)
            border: none
            padding: 0
            a
                padding: 0
            .rkg-profile-meni-close
                @mixin x $color-main, 7vw, 0.5vw
                @util position(absolute, 5vw 5vw null null)
        li
            a
                padding-left: 12vw
        > .rkg-profile-meni-item-group
            padding: 1
            margin-bottom: 10vh
        .rkg-profile-meni-messages
            min-height: 53vh

.rkg-profile-meni-item
    .hover
        display: none
    &:hover
        .no-hover
            display: none
        .hover
            display: inline

.rkg-profile-meni-item-fixed
    @media (--mobile)
        @util position(fixed, null 0 0 0)
        background: white
        &::before
            @util position(absolute, -7.1vh 0 null 0)
            height: 7vh
            background: linear-gradient(to bottom, color(white a(0)) 0%, color(white a(100%)) 100%)
            border-bottom: 1px solid $color-main
            content: ''
            pointer-events: none

.dashicons-before::before
    width: 1.1vw !important
    height: 1.1vw !important
    font-size: 1.1vw !important
    @media (--mobile)
        width: 3vh !important
        height: 3vh !important
        font-size: 14px !important

.power
    @mixin power $color-title, 1.1vw, 0.15vw
    display: inline-block
    padding: 0 0.5vw 0 0
    @media (--mobile)
        @mixin power $color-title, 4.5vw, 0.5vw
        padding: 0 6vw 0 0

#nav-main
    ul
        margin: 0
        padding: 0
        list-style: none
        li
            margin-bottom: 1.3vw
            list-style: none
            text-align: center
            a
                color: $color-header
                text-decoration: none
                font-weight: $regular
                font-size: 2.2rem
                &:hover
                    color: color($color-header a(80%))
            @media (--mobile)
                margin-bottom: 2vh
                a
                    font-size: 23px

.rkg-header
    position: relative
    overflow: hidden
    max-height: 100vh
    width: 100%
    height: 100vh
    background-image: url('assets/img/hero.jpg')
    background-position: center top
    background-size: cover
    color: $color-header
    @media (--mobile)
        background-image: url('assets/img/hero-mobile.webp')
        background-position: center bottom

.rkg-header-gradient
    @util position(absolute, 0 0 0 0)
    background: linear-gradient(to bottom, color(black a(0)) 99%, color(black a(50%)) 100%)

.rkg-footer
    margin-top: 15vw
    width: 100%
    background-position: center center
    background-size: cover
    color: $color-header
    text-align: center
    background-image: url('assets/img/footer.jpg')

.rkg-footer-gradient
    @util size(100%)
    background: linear-gradient(to bottom, color(black a(50%)) 0, color(black a(0)) 2%)

.rkg-footer-title
    margin-bottom: 4vw
    padding-top: 6vw
    text-transform: uppercase
    font-weight: $regular
    font-size: 2.5rem
    @media (--mobile)
        margin-bottom: 6vh
        padding-top: 25vh
        font-size: 6vw
        p
            margin-bottom: 3.5vh

.rkg-footer-text
    margin-bottom: 4vw
    font-weight: $regular
    font-size: 1.1rem
    @media (--mobile)
        margin-bottom: 6vh
        font-size: 2.3vw

.rkg-footer-external
    margin-bottom: 3vw
    a
        margin: 0 2.5vw
        color: $color-header
        font-size: 3rem
        opacity: 0.5
        &:hover
            color: color($color-header a(50%))
    @media (--mobile)
        margin-bottom: 7vh
        a
            margin: 0 5vw
            font-size: 8vw

.rkg-footer-copy
    padding-bottom: 0.5vw
    font-weight: $light
    font-size: 0.8rem
    opacity: 0.5
    @media (--mobile)
        padding-bottom: 3vh
        font-size: 1.8vw

.rkg-top-stick
    position: relative
    z-index: 1005
    height: 8vw
    pointer-events: none
    *
        pointer-events: auto
    @media (--mobile)
        height: 18vw
        background: white

.sticky
    @util position(fixed, 0 0 null 0)

.sticky + #content
    padding-top: 8vw
    @media (--mobile)
        padding-top: 18vw

.rkg-front-hide
    display: none

.rkg-top-logo
    margin-left: 2.5vw
    height: 8vw
    @media (--mobile)
        height: 18vw

#rkg-top-meni
    @util position(absolute, 2.5vw 2.5vw null null)
    i
        margin-left: 2.5vw
        color: $color-title
        font-size: 2.5rem
        cursor: pointer
        &:hover
            color: color($color-title a(80%))
    .rkg-meni-open
        margin-left: 2.5vw
    @media (--mobile)
        top: 5vw
        right: 5vw
        i
            font-size: 7vw

.rkg-header-meni
    @util position(absolute, 2.5vw 2.5vw null null)
    i
        margin-left: 2.5vw
        font-size: 2.5rem
        cursor: pointer
        &:hover
            color: color(white a(80%))
    .rkg-meni-open
        margin-left: 2.5vw
    .rkg-meni-close
        @mixin x white
    @media (--mobile)
        top: 5vw
        right: 5vw
        i
            font-size: 7vw
        .rkg-meni-close
            @mixin x white, 7vw, 0.5vw

.rkg-header-content
    @util position(absolute, 23vh 0 0 0)
    @util size(64vw 22vw)
    margin: 0 auto
    @media (--mobile)
        @util position(absolute, 11vh 0 0 0)
        @util size(76vw auto)

.rkg-header-logo
    display: inline-block
    width: 22vw
    img
        width: 22vw
        opacity: 0.3
    @media (--mobile)
        display: block
        margin: auto
        width: 55vw
        img
            width: 55vw

.rkg-header-title
    float: right
    margin-top: 2.1vw
    margin-left: 2.5vw
    width: 39.5vw
    @media (--mobile)
        float: initial
        margin-top: 25vh
        margin-left: initial
        width: initial

.rkg-header-s1
    display: inline-block
    margin-bottom: 1.1vw
    font-weight: $bold
    font-size: 1.6rem
    @media (--mobile)
        display: block
        margin-bottom: 3vw
        font-size: 3.2vw

.rkg-header-s2
    display: inline-block
    margin-bottom: 1.5vw
    font-weight: $medium
    font-size: 3.4rem
    @media (--mobile)
        display: block
        margin-bottom: 5vw
        font-size: 6vw

.button-title
    @util size(13vw, 3.8vw)
    padding: 0.5vw
    border: 0.15vw solid white
    border-radius: 1.9vw
    background: none
    color: white
    font-weight: $semi-bold
    font-size: 1.3rem
    cursor: pointer
    &:first-of-type
        margin-right: 5vw
    &:hover
        background: white
        color: $color-title
    &:active
        border-color: color(white shade(15%))
        background: color(white shade(15%))
        color: $color-title
    @media (--mobile)
        @util size(35vw, 10vw)
        padding: 0.5vw
        border: 0.4vw solid white
        border-radius: 5vw
        font-size: 4vw
        &:first-of-type
            margin-right: 6vw

#rkg-header-arrow-container
    @util position(absolute, null null 1.5vw null)
    @util size(2.5vw)
    left: 50vw
    margin-left: -1.125vw
    @media (--mobile)
        @util size(7vw)
        bottom: 4vw
        margin-left: -3.5vw

.rkg-header-arrow
    @mixin arrow bottom, white
    @media (--mobile)
        @mixin arrow bottom, white, 7vw, 0.5vw

#content
    width: 100%

.article-content
    > .article-body > *,
    > .rkg-article-meta
        margin: 0 auto 2vw
        width: 75vw
        @media (--mobile)
            margin-bottom: 4vw
            width: 90vw
    > .article-h1
        margin: 7vw auto 4.5vw
        width: 75vw
        @media (--mobile)
            width: 90vw
            margin-top: 18vw
            margin-bottom: 4vw
    > .article-body > h2
        margin-top: 17vw
        @media (--mobile)
            margin-top: 18vw
    > .article-body > h2#rkg-whoarewe
        margin-top: 7vw
    > .article-body > h4
        margin-bottom: 1vw
    p
        line-height: 2vw
        @media (--mobile)
            color: $color-main-mobile
            line-height: 150%
    .rkg-map-container
        position: relative
        margin: 0 auto 4.5vw
        width: 100%
        @media (--mobile)
            width: 100%
            margin-bottom: 4vw
    > .article-body > .rkg-ancor
        display: block
        margin: 12vw 0 0 0
        height: 1px
        @media (--mobile)
            margin: 0 0 18vw 0
    > .article-body > .rkg-ancor + h2
        margin-top: 5vw
    > .article-body a.rkg-button,
    > .article-body button.rkg-button
        font-weight: $medium
        color: $color-main
        font-size: 0.9rem
        padding: 1vw
        cursor: pointer
        margin: 1vw 1vw 0 1vw
        width: 10vw
        border: 1px solid $color-title
        border-radius: 5vw
        &:hover
            background: $color-title
            color: white
        &:active
            background: color($color-title tint(15%))
        @media (--mobile)
            @util size(35vw, 10vw)
            font-size: 14px
            padding: 1vh 0.5vw
            border: 0.4vw solid $color-title
            border-radius: 5vw
            display: inline-block
            cursor: pointer
            box-sizing: border-box
    input,
    select
        font-size: 0.9rem
        padding: 1vw
        width: 20vw
        border: 1px solid color($color-main tint(50%))
        &::placeholder
            color: $color-main
            font-weight: $medium
        @media (--mobile)
            font-size: 14px
    option:disabled
        color: $color-main

.article-content-narrow
    > .article-body > *,
    > .rkg-article-meta,
    > .article-h1
        margin: 0 40vw 4.5vw 12.5vw
        width: 47vw
        @media (--mobile)
            margin: 0 auto 12vw
            width: 90vw !important
    @media (--mobile)
        display: flex
        flex-direction: column
        > .article-body
            order: 3
    > .article-body > h2
        margin-top: 7vw
        @media (--mobile)
            margin-top: 18vw
    > .article-body > h4
        margin-bottom: 1vw
    > .rkg-map-container
        margin: 0 auto 4.5vw
        width: 100%
        @media (--mobile)
            width: 100%
            margin-bottom: 4vw

.article-body > p,
.article-body > ul li
    color: $color-reading-font
    margin-bottom: 1vw !important
    font-size: 0.8em
    line-height: 1.6em
    @media (--mobile)
        font-size: 1.35em 
        line-height: 1.6em
        margin-bottom: 3vw !important

.article-body > figure
    display: inline-block
    margin-right: 5vw !important
    max-width: 100%
    @media (--mobile)
        margin-right: auto !important
        display: block

.article-h1
    text-transform: uppercase
    color: $color-title
    font-weight: $regular
    font-size: 3rem
    @media (--mobile)
        order: 1
        font-size: 23px

.article-body
    word-wrap: break-word

    > p
        overflow: hidden
    
    > h2
        color: $color-title
        font-weight: $regular
        font-size: 3rem
        text-transform: uppercase
        @media (--mobile)
            font-size: 23px
    a
        color: $color-title
        text-decoration: none

.rkg-button
    font-weight: $medium
    color: $color-main
    font-size: 0.9rem
    padding: 1vw
    cursor: pointer
    margin: 1vw 1vw 0 1vw
    width: 10vw
    border: 1px solid $color-title
    border-radius: 5vw
    &:hover
        background: $color-title
        color: white
    &:active
        background: color($color-title tint(15%))
    @media (--mobile)
        @util size(35vw, 10vw)
            font-size: 14px
        padding: 1vh 0.5vw
        border: 0.4vw solid $color-title
        border-radius: 5vw
        display: inline-block
        cursor: pointer
        box-sizing: border-box

.rkg-side
    float: right
    margin-right: 12.5vw
    margin-bottom: 2vw
    width: 21vw
    @util clearfix
    ul.menu
        list-style: none
    ul
        margin-top: 0
        margin-bottom: 1vw
        li
            display: block
            min-height: 1.2vw
            font-size: 1.1rem
            a
                display: block
                margin-bottom: 0.5vw
                color: black
                text-decoration: none
                text-transform: uppercase
                font-weight: $bold
                &:hover
                    color: color(black tint(30%))
            ul
                list-style: none
                padding-left: 0
                li
                    a
                        margin-bottom: 0.5vw
                        color: $color-main
                        text-transform: none
                        font-weight: $semi-bold
                        &:hover
                            color: color($color-main tint(30%))
    h2
        margin-bottom: 1vw
        color: black
        text-transform: uppercase
        font-weight: $bold
        font-size: 1.2rem
    p
        margin-bottom: 1vw
        line-height: 1.5vw
        font-size: 1.2rem
        select,
        input
            width: 100%
            box-sizing: border-box
            @media (--mobile)
                padding: 4vw
    a
        color: $color-title
        text-decoration: none
        font-size: 1.2rem
    .rkg-user
        line-height: 1.5vw
        font-size: 1.2rem
        white-space: nowrap
        @media (--mobile)
            margin-top: 14px
            line-height: 18px
            font-size: 14px
    .excursion-prevnext-prev,
    .excursion-prevnext-next
        display: block
        .dashicons
            font-size: 4rem
        @media (--mobile)
            .dashicons
                font-size: 48px
    &::after
        clear: both
    @media (--mobile)
        order: 2
        float: none
        margin: 0 auto 12vw
        width: 95vw
        ul
            padding-left: 0
            li
                margin-bottom: 2vw
                min-height: 2.6vw
                font-size: 14px
        h2,
        p,
        a
            font-size: 1.2em
            line-height: 1.5em

.excursion-prevnext-prev
    float: left

.excursion-prevnext-next
    float: right

.excursion-prevnext
    margin-bottom: 24vw

.wp-block-image
    text-align: center

.course-block-container
    box-sizing: border-box
    padding: 0 2vw 0 0
    @media (--mobile)
        padding: 0 12.5vw

.course-block-h
    margin-top: 1vw
    color: $color-title
    text-transform: uppercase
    font-weight: $bold
    font-size: 0.9rem
    @media (--mobile)
        margin-bottom: 1vw
        font-size: 14px

.course-block-h2
    margin-bottom: 1vw
    color: $color-title
    text-transform: uppercase
    font-weight: $bold
    font-size: 0.9rem
    @media (--mobile)
        margin-bottom: 3vw
        font-size: 14px

.course-block-date
    color: $color-title
    font-weight: $regular
    font-size: 7rem
    text-align: center
    .dashicons
        font-size: 7vw
        width: 100%
        height: 7vw
        margin-top: 1vw
    img
        width: 8vw
        height: 8vw
        margin: 0.8vw auto
        display: block
    @media (--mobile)
        margin-bottom: 1vw
        font-size: 90px
        .dashicons
            font-size: 90px
        img
            width: 90px
            height: 90px

